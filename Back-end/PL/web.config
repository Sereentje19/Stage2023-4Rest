<?xml version="1.0" encoding="utf-8"?>
<configuration>
		<system.webServer>
			<directoryBrowse enabled="false" />
			<handlers>
				<add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
			</handlers>
			<aspNetCore processPath="dotnet" arguments="PL.dll" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" />
			<security>
				<authentication>
					<anonymousAuthentication enabled="false" />
					<windowsAuthentication enabled="false" />
				</authentication>
			</security>
		</system.webServer>
</configuration>